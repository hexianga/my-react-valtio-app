{
  "name": "react-valtio-app",
  "version": "1.0.0",
  "description": "React application with Valtio, TypeScript, TailwindCSS and Sentry",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack --config config/webpack.dev.js",
    "build:test": "cross-env NODE_ENV=test webpack --config config/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug --project=chromium",
    "test:report": "playwright show-report",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "start": "npm run dev"
  },
  "keywords": [
    "react",
    "typescript",
    "valtio",
    "tailwindcss",
    "sentry"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@formily/antd-v5": "^1.2.4",
    "@formily/core": "^2.3.7",
    "@formily/react": "^2.3.7",
    "@formily/validator": "^2.3.7",
    "@sentry/react": "^7.80.1",
    "@sentry/tracing": "^7.80.1",
    "ahooks": "^3.7.8",
    "antd": "^5.22.6",
    "axios": "^1.6.2",
    "dayjs": "^1.11.18",
    "diff": "^8.0.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.20.1",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "valtio": "^1.12.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^10.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.6.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
